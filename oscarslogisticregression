col.endswith('_estimated')]\n","\n","# Iterate through estimated columns\n","for col in estimated_columns:\n","    # Extract actual and estimated values\n","    actual_values = result_df_modified[col.replace('_estimated', '')]  # Get corresponding column without '_estimated'\n","    estimated_values = result_df_modified[col]\n","\n","    # Find films that were nominated but not in the estimated values\n","    actual_nominated_not_estimated = result_df_modified.loc[(actual_values == 1) & (estimated_values == 0), ['primaryTitle', 'releaseYear']]\n","    # Find films that were nominated in estimated values but not actually nominated\n","    estimated_nominated_not_actual = result_df_modified.loc[(actual_values == 0) & (estimated_values == 1), ['primaryTitle', 'releaseYear']]\n","    estimated_nominated_and_actual = result_df_modified.loc[(actual_values == 1) & (estimated_values == 1), ['primaryTitle', 'releaseYear']]\n","\n","    # Display the results\n","    print(f\"{col.replace('_estimated', '')}: Films correctly identified as nominated\")\n","    print(estimated_nominated_and_actual)\n","    print(\"-\" * 40)\n","    print(f\"{col.replace('_estimated', '')}: Films that were nominated that we did not predict\")\n","    print(actual_nominated_not_estimated)\n","    print(\"-\" * 40)\n","    print(f\"{col.replace('_estimated', '')}: Films that we predicted to be nominated but were not\")\n","    print(estimated_nominated_not_actual)\n","    print(\"-\" * 60)\n"]}],"metadata":{"colab":{"provenance":[]},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}