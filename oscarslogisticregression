top_indices = result_df_modified.groupby('releaseYear')[col].nlargest(top_n).index\n","        # Update the values based on the top indices\n","        result_df_modified[col] = 0\n","        result_df_modified.loc[top_indices.get_level_values(1), col] = 1\n","\n","# Display the modified DataFrame\n","print(result_df_modified)\n","\n","# Display the modified DataFrame\n","print(result_df_modified)\n"]},{"cell_type":"code","execution_count":18,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":224,"status":"ok","timestamp":1702402594043,"user":{"displayName":"Victor Wu","userId":"17032740492318721266"},"user_tz":480},"id":"FmUKzl934t-s","outputId":"3795456d-91b5-4e48-8ddd-2198bf978a98"},"outputs":[{"output_type":"stream","name":"stdout","text":["\n","==================== Classification Report for CINEMATOGRAPHY ====================\n","Category             Precision  Recall     F1-Score  \n","0                    1.00       1.00       1.00      \n","1                    0.40       0.48       0.43      \n","------------------------------------------------------------\n","\n","==================== Classification Report for DIRECTING ====================\n","Category             Precision  Recall     F1-Score  \n","0                    1.00       1.00       1.00      \n","1                    0.64       0.70       0.67      \n","------------------------------------------------------------\n","\n","==================== Classification Report for FILM EDITING ====================\n","Category             Precision  Recall     F1-Score  \n","0                    1.00       1.00       1.00      \n","1                    0.60       0.65       0.63      \n","------------------------------------------------------------\n","\n","==================== Classification Report for ACTOR IN A SUPPORTING ROLE ====================\n","Category             Precision  Recall     F1-Score  \n","0                    1.00       1.00       1.00      \n","1   